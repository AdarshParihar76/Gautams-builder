<%- include("partials/header") %>

<div class="container">

	<div class="container border border-primary rounded px-1 mt-3" style="background-color: rgba(255,255,220,.8)">
		<h1> Gautams Builder <br> <em id="and"> & </em> </br> <b>Vaishnavi</b> <img id="logo" src="logos/logo.png">ons. </h1>
		<p id="tag"> ( WE MAKE YOUR DREAMS HOME ) </p>
	</div>
</div>

<div class="container text-light mt-5 p-3 rounded" style="background-color: rgba(15,10,20,.7)">
	<% rb.forEach(function(b){ %>
		<% if(b.status == null){ %>
				<div>
					<h2> Non-Running booking : <%= b.name %></h2><br>
					<a class="btn btn-info float-right" href="/<%= b._id %>/rbookingDetails"> View </a><br/>
					<hr>
				</div>
			<% } else{ %>
			<div>
				<h2><%= b.status %>. <%= b.name %></h2><br>
				<a class="btn btn-info float-right" href="/<%= b._id %>/rbookingDetails"> View </a><br/>
				<hr>
			</div>
			<% } %>
	<% }); %>
</div>


<%- include("partials/footer") %>